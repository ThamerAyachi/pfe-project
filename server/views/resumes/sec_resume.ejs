<!DOCTYPE html>
<html>
  <head>
    <title>Web Developer Resume</title>
    <style>
      body {
        font-family: Arial, sans-serif;
        margin: 20px;
      }

      h1,
      h2,
      h3,
      h4 {
        margin: 0;
      }

      h1 {
        font-size: 24px;
        font-weight: bold;
      }

      h2 {
        font-size: 18px;
        color: #888;
      }

      h3 {
        font-size: 16px;
        font-weight: bold;
      }

      p {
        margin: 5px 0;
      }

      ul {
        list-style-type: disc;
        margin: 5px 0 10px 20px;
      }

      a {
        color: #007bff;
        text-decoration: none;
      }

      section {
        margin-bottom: 20px;
      }

      article {
        margin-bottom: 10px;
      }
    </style>
  </head>
  <body>
    <header>
      <h1>SUNSHINE SMITH</h1>
      <h2>WEB DEVELOPER</h2>
    </header>

    <section id="contact">
      <h3>CONTACT</h3>
      <p>Email: <%= contact.email %></p>
      <p>Phone: <%= contact.phone %></p>
      <p>Location: <%= contact.location %></p>
      <p>LinkedIn: <a href="<%= contact.linkedin %>">LinkedIn Profile</a></p>
    </section>

    <section id="objective">
      <h3>CAREER OBJECTIVE</h3>
      <p><%= careerObjective %></p>
    </section>

    <section id="education">
      <h3>EDUCATION</h3>
      <p><%= education.degree %></p>
      <p><%= education.major %></p>
      <p><%= education.university %></p>
      <p><%= education.years %></p>
      <p><%= education.location %></p>
    </section>

    <section id="skills">
      <h3>SKILLS</h3>
      <ul>
        <% skills.forEach(skill => { %>
        <li><%= skill %></li>
        <% }); %>
      </ul>
    </section>

    <section id="experience">
      <h3>WORK EXPERIENCE</h3>

      <% workExperience.forEach(experience => { %>
      <article>
        <h4><%= experience.position %></h4>
        <p><%= experience.company %></p>
        <p><%= experience.years %></p>
        <ul>
          <% experience.responsibilities.forEach(responsibility => { %>
          <li><%= responsibility %></li>
          <% }); %>
        </ul>
      </article>
      <% }); %>
    </section>
  </body>
</html>
